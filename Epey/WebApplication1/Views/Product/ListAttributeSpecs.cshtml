@{
}
@model IEnumerable<WebApplication1.Models.AttributeSpec>

<ul class="max-w-xs flex flex-col">
    
    @foreach (var item in Model)
    {
       
        <li class="inline-flex items-center gap-x-2 py-3 px-4 text-sm font-medium odd:bg-orange-100 bg-white border border-gray-200 text-gray-800 -mt-px first:rounded-t-lg first:mt-0 last:rounded-b-lg">
            @Html.DisplayFor(modelItem => item.AttributeSpecsName)

        </li>
    }
    
</ul>

